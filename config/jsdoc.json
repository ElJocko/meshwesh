{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./"],
    "includePattern": ".+\\.js(x|doc)?$",
    "exclude": [
      ".git",
      "node_modules",
      "public/vendor",
      "docs"
    ]
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "systemName": "Meshwesh",
    "footer": "Washington Grand Company",
    "copyright": "2017",
    "navType": "vertical",
    "theme": "cosmo",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "private": true
  }
}